[report]
fail_under = 100
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:

[run]
branch = True
source =
    tags_input
    example

omit =
    */mock/*
    */django/*
    */tests/*
    */example/*
    */metadata.py

[paths]
source =
    tags_input
    example
    example.tests

